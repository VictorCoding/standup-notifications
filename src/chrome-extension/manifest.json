{
  "manifest_version": 2,

  "name": "Stand Up Notifications",
  "description": "This extension notifies the user when to stand up",
  "version": "1.0",

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}